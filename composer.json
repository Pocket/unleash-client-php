{
  "name": "unleash/client",
  "description": "Unleash Client for PHP",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": "^5.6.0",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "lastguest/murmurhash": "^1.3.0",
    "symfony/http-foundation": "~3.4.0",
    "symfony/event-dispatcher": "~3.4.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "autoload": {
    "files": [
      "src/Utils/normalizer.php"
    ],
    "psr-4": {
      "Unleash\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "5.6",
    "mockery/mockery": "^1.2"
  }
}
